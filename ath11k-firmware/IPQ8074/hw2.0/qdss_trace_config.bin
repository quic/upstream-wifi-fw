//This file is used to configure MAC tracer eventmasks for QDSS enabled.
//For QDSS disabled, the tracer is configured from settings at 
//whal_mac_tracer.c (default_mac_tracer_module and whal_mac_tracer_lmac_cfg_event_mask).
//Keep this file (qdss_trace_config.bin) and whal_mac_tracer.c as same for maintainability.

//This is a commented line and parser will ignore it.
//Tracer scenario(or commands) always starts with start keyword
//Below commands to enable Event traces on UMAC,MAC0,MAC1
seq_start;
seq_type:mem_req;
//memory size always given in KB. beow example reserves 3 buffers segments of 16 KB.
sink:etr_ddr,0x1,0x400;
seq_end;
seq_start;
seq_type:mac_event_trace;
sink:etr_ddr;
//LMAC0
subsys_cfg_start:mac0;
//sw events always has module id as zero.
swevt:0x0, 0xFFFFFFFF,0xFFFFFFFF,0x0,0x0;
//modify the modules and eventmasks below for mac0 (refer hw_trc_cfg_parser.c and whal_mac_tracer.c).
rxpcu:0x1, 0x00000217, 0x80008000, 0x00000000, 0x00000000;
rxdma:0x2, 0x3F37FE7F, 0x00017F04, 0x00000000, 0x00000000;
//crypto:0x3, 0xf79B3770, 0x00000000, 0x00000000, 0x00000000;
//pdg:0x3, 0x8E060F07, 0x00000000, 0x00000000, 0x00000000;
txpcu:0x3, 0xF6C7B4C7, 0x000000D2, 0x00000000, 0x00000000;
txole:0x4, 0x601F0EB4, 0x6FF0F000, 0x00000000, 0x00000000;
hwsch:0x5, 0xECD631, 0x10000004, 0x00000000, 0x00000000;
subsys_cfg_end:mac0;
//LMAC1
subsys_cfg_start:mac1;
swevt:0x0, 0xFFFFFFFF,0xFFFFFFFF,0x0,0x0;
rxpcu:0x6, 0x00000217, 0x80008000, 0x00000000, 0x00000000;
rxdma:0x7, 0x3F37FE7F, 0x00017F04, 0x00000000, 0x00000000;
//crypto:0x8, 0xf79B3770, 0x00000000, 0x00000000, 0x00000000;
//pdg:0x8, 0x8E060F07, 0x00000000, 0x00000000, 0x00000000;
txpcu:0x8, 0xF6C7B4C7, 0x000000D2, 0x00000000, 0x00000000;
txole:0x9, 0x601F0EB4, 0x6FF0F000, 0x00000000, 0x00000000;
hwsch:0xa, 0xECD631, 0x10000004, 0x00000000, 0x00000000;
subsys_cfg_end:mac1;
//LMAC2
subsys_cfg_start:mac2;
swevt:0x0, 0xFFFFFFFF,0xFFFFFFFF,0x0,0x0;
rxpcu:0xb, 0x00000217, 0x80008000, 0x00000000, 0x00000000;
//rxdma:0xc, 0x3F37FE7F, 0x00017F04, 0x00000000, 0x00000000;
//crypto:0xc, 0xf79B3770, 0x00000000, 0x00000000, 0x00000000;
//pdg:0xc, 0x8E060F07, 0x00000000, 0x00000000, 0x00000000;
txpcu:0xc, 0xF6C7B4C7, 0x000000D2, 0x00000000, 0x00000000;
txole:0xd, 0x601F0EB4, 0x6FF0F000, 0x00000000, 0x00000000;
hwsch:0xe, 0xECD631, 0x10000004, 0x00000000, 0x00000000;
subsys_cfg_end:mac2;
//UMAC - uncomment lines "subsys_cfg_start:umac;" and "subsys_cfg_end:umac;".
subsys_cfg_start:umac;
tqm:0xf, 0x00000000, 0x87F00000, 0x00000000, 0x00000000;
subsys_cfg_end:umac;
swap:0xFFFFFFFF;
trigger_start:trc;
wfi:0xFFFFFFFF,0xFFFFFFFF,0xFFFFFFFF,0xFFFFFFFF;
ts0:0xFFFFFFFF,0xFFFFFFFF,0xFFFFFFFF,0xFFFFFFFF;
ts1:0xFFFFFFFF,0xFFFFFFFF,0xFFFFFFFF,0xFFFFFFFF;
ts2:0xFFFFFFFF,0xFFFFFFFF,0xFFFFFFFF,0xFFFFFFFF;
ts3:0xFFFFFFFF,0xFFFFFFFF,0xFFFFFFFF,0xFFFFFFFF;
ts4:0xFFFFFFFF,0xFFFFFFFF,0xFFFFFFFF,0xFFFFFFFF;
trigger_end:trc;
memw:0xFFFFFFFF,0xFFFFFFFF;
seq_end;
